{
  "model_architecture": "CrossAttentionModel",
  "feature_extractor": "BiomedCLIP-PubMedBERT_256-vit_base_patch16_224",
  "embedding_dimension": 768,
  "cross_attention_layers": 6,
  "attention_heads": 8,
  "dropout": 0.1,
  "training_config": {
    "loss_function": "CustomSupConLoss",
    "temperature": 0.07,
    "optimizer": "AdamW",
    "learning_rate": 0.0001,
    "weight_decay": 1e-05,
    "scheduler": "CosineAnnealingLR",
    "max_epochs": 15,
    "early_stopping_patience": 5
  }
}